<div class="container">
  <%=render 'layouts/list',user: current_user, post: Post.new%>
  <div>
    <h2>利用者</h2>
    <table>
      <thead>
        <tr>
          <th>画像</th>
          <th>名前</th>
          <th colspan="3"></th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user|%>
        <tr>
          <td><%=image_tag user.get_profile_image(50,50)%></td>
          <td><%=user.name%></td>
          <td><%=link_to '詳細へ',user_path(user)%></td>
        </tr>
        <%end%>
      </tbody>
    </table>
  </div>
</div>